<%#profile view file %>
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Profile View</title>
  </head>
  <body>
    <header><%- include('header.ejs') %></header>
    <h1>Profile</h1>
    <div>
      <p><%= user.name %></p>
      <p><%= user.username %></p>
      <p><%= user.phone %></p>
      <p><%= user.email %></p>
      <p><%= user.address %></p>
    </div>
    <br />
    <a href="/">Back to Account summary</a>
    <footer><%- include("footer.ejs") %></footer>
  </body>
</html>
